<main>
    <div class='{{styles.profile}}'>
        {{{back}}}
        <div class='{{styles.profile__info}}'>
            {{#if isLoading }}
                <h1>Загружаем...</h1>
            {{ else }}
            <div>
                {{{ form }}}
                <div class="{{styles.profile__form-footer}}">
                    {{#each buttons}}
                        {{{ this }}}
                    {{/each}}
                </div>
            </div>
        </div>
    </div>
    {{/if}}
</main>

